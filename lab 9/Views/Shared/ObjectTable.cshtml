@model List<ObjectItem>

@if (Model != null && Model.Count > 0)
{
    <table border="1">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
            </tr>
        }
    </table>
}
else
{
    <p>No items to display.</p>
}

